		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"globals": "^15.12.0",
		"prettier": "^3.4.1",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte": "^5.2.10",
		"svelte-check": "^4.1.0",
		"tailwindcss": "^3.4.15",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.16.0",
		"vite": "^5.4.11",
