{
	"name": "web",
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": "20.x"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
